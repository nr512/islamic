[
    {
      "text": "الدنيا متاع، وخير متاعها المرأة الصالحة",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "خير الناس أنفعهم للناس",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "إنما الأعمال بالنيات",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "من حسن إسلام المرء تركه ما لا يعنيه",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "لا تدخلون الجنة حتى تؤمنوا، ولا تؤمنوا حتى تحابوا",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الكلمة الطيبة صدقة",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "اتق الله حيثما كنت",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "لا تغضب ولك الجنة",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "ازهد في الدنيا يحبك الله، وازهد فيما عند الناس يحبك الناس",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "إن الله جميل يحب الجمال",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الدين المعاملة",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "خيركم خيركم لأهله",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الصبر ضياء",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "من صمت نجا",
      "author": "الإمام علي رضي الله عنه"
    },
    {
      "text": "أفضل الذكر الخفي، وخير الرزق ما يكفي",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "لا تنظر إلى صغر الخطيئة، ولكن انظر إلى عظم من عصيت",
      "author": "الإمام البصري"
    },
    {
      "text": "من عرف نفسه عرف ربه",
      "author": "الإمام علي رضي الله عنه"
    },
    {
      "text": "قيمة كل امرئ ما يحسنه",
      "author": "الإمام علي رضي الله عنه"
    },
    {
      "text": "العلم خير من المال، العلم يحرسك وأنت تحرس المال",
      "author": "الإمام علي رضي الله عنه"
    },
    {
      "text": "لا تؤجل عمل اليوم إلى الغد",
      "author": "عمر بن الخطاب رضي الله عنه"
    },
    {
      "text": "من جد وجد",
      "author": "حكمة عربية"
    },
    {
      "text": "خير الكلام ما قل ودل",
      "author": "حكمة عربية"
    },
    {
      "text": "الوقت كالسيف إن لم تقطعه قطعك",
      "author": "حكمة عربية"
    },
    {
      "text": "العلم نور",
      "author": "حكمة عربية"
    },
    {
      "text": "اطلبوا العلم من المهد إلى اللحد",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "خير جليس في الزمان كتاب",
      "author": "المتنبي"
    },
    {
      "text": "لكل مجتهد نصيب",
      "author": "حكمة عربية"
    },
    {
      "text": "رأس الحكمة مخافة الله",
      "author": "حكمة عربية"
    },
    {
      "text": "من سار على الدرب وصل",
      "author": "حكمة عربية"
    },
    {
      "text": "الصبر مفتاح الفرج",
      "author": "حكمة عربية"
    },
    {
      "text": "العقل السليم في الجسم السليم",
      "author": "حكمة عربية"
    },
    {
      "text": "يد الله مع الجماعة",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "خير البر عاجله",
      "author": "حكمة عربية"
    },
    {
      "text": "المؤمن مرآة أخيه",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الجار قبل الدار",
      "author": "حكمة عربية"
    },
    {
      "text": "من طلب العلا سهر الليالي",
      "author": "حكمة عربية"
    },
    {
      "text": "الجنة تحت أقدام الأمهات",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "العلم في الصغر كالنقش على الحجر",
      "author": "حكمة عربية"
    },
    {
      "text": "اليد العليا خير من اليد السفلى",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "خير الأمور أوسطها",
      "author": "حكمة عربية"
    },
    {
      "text": "الرفق ما كان في شيء إلا زانه",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الدال على الخير كفاعله",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "من تواضع لله رفعه",
      "author": "حكمة عربية"
    },
    {
      "text": "الجزاء من جنس العمل",
      "author": "حكمة عربية"
    },
    {
      "text": "الندم توبة",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "المسلم من سلم المسلمون من لسانه ويده",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "أحب الأعمال إلى الله أدومها وإن قل",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "خير الأصحاب عند الله خيرهم لصاحبه",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الحياء من الإيمان",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "الساعي على الأرملة والمسكين كالمجاهد في سبيل الله",
      "author": "النبي محمد ﷺ"
    },
    {
      "text": "من حفظ لسانه أراح نفسه",
      "author": "حكمة عربية"
    },
    {
      "text": "لا راحة للحسود",
      "author": "حكمة عربية"
    },
    {
      "text": "الصديق وقت الضيق",
      "author": "حكمة عربية"
    },
    {
      "text": "الكتاب خير جليس",
      "author": "حكمة عربية"
    },
    {
      "text": "العلم سلاح ذو حدين",
      "author": "حكمة عربية"
    },
    {
      "text": "الأدب خير ميراث",
      "author": "حكمة عربية"
    },
    {
      "text": "الوقاية خير من العلاج",
      "author": "حكمة عربية"
    },
    {
      "text": "من جالس العلماء وقر",
      "author": "حكمة عربية"
    },
    {
      "text": "البلاء موكل بالمنطق",
      "author": "حكمة عربية"
    },
    {
      "text": "لا حياء في الدين",
      "author": "حكمة عربية"
    },
    {
      "text": "العين بصيرة واليد قصيرة",
      "author": "حكمة عربية"
    },
    {
      "text": "الحق أبلج",
      "author": "حكمة عربية"
    },
    {
      "text": "لا تؤخر عمل اليوم إلى الغد",
      "author": "حكمة عربية"
    },
    {
      "text": "من زرع حصد",
      "author": "حكمة عربية"
    },
    {
      "text": "كل إناء بما فيه ينضح",
      "author": "حكمة عربية"
    },
    {
      "text": "إذا تم العقل نقص الكلام",
      "author": "حكمة عربية"
    },
    {
      "text": "المرء بأصغريه: قلبه ولسانه",
      "author": "حكمة عربية"
    },
    {
      "text": "العتاب هدية الأحباب",
      "author": "حكمة عربية"
    },
    {
      "text": "ما خاب من استشار",
      "author": "حكمة عربية"
    },
    {
      "text": "الصمت حكمة وقليل فاعله",
      "author": "حكمة عربية"
    },
    {
      "text": "من صبر ظفر",
      "author": "حكمة عربية"
    },
    {
      "text": "الحلم سيد الأخلاق",
      "author": "حكمة عربية"
    },
    {
      "text": "الصدق منجاة",
      "author": "حكمة عربية"
    },
    {
      "text": "العقل زينة",
      "author": "حكمة عربية"
    },
    {
      "text": "القناعة كنز لا يفنى",
      "author": "حكمة عربية"
    },
    {
      "text": "أدب المرء خير من ذهبه",
      "author": "حكمة عربية"
    },
    {
      "text": "الجار للجار وإن جار",
      "author": "حكمة عربية"
    },
    {
      "text": "الباب الذي يأتيك منه الريح سده واسترح",
      "author": "حكمة عربية"
    },
    {
      "text": "العلم حياة القلوب",
      "author": "حكمة عربية"
    },
    {
      "text": "الكلام كالدواء، قليله ينفع وكثيره قاتل",
      "author": "حكمة عربية"
    },
    {
      "text": "السكوت من ذهب والكلام من فضة",
      "author": "حكمة عربية"
    },
    {
      "text": "البر لا يبلى والذنب لا ينسى",
      "author": "حكمة عربية"
    },
    {
      "text": "من حفر حفرة لأخيه وقع فيها",
      "author": "حكمة عربية"
    },
    {
      "text": "الحق يعلو ولا يعلى عليه",
      "author": "حكمة عربية"
    },
    {
      "text": "العز في نواصي الخيل",
      "author": "حكمة عربية"
    },
    {
      "text": "الصبر عند الصدمة الأولى",
      "author": "حكمة عربية"
    },
    {
      "text": "تجري الرياح بما لا تشتهي السفن",
      "author": "حكمة عربية"
    },
    {
      "text": "رب كلمة تقول لصاحبها دعني",
      "author": "حكمة عربية"
    },
    {
      "text": "عند الامتحان يكرم المرء أو يهان",
      "author": "حكمة عربية"
    },
    {
      "text": "من خاف سلم",
      "author": "حكمة عربية"
    },
    {
      "text": "أسعد الناس أحسنهم خلقاً",
      "author": "حكمة عربية"
    },
    {
      "text": "النظافة من الإيمان",
      "author": "النبي محمد
    }
]
